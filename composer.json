{
    "name": "mckltech/close-php",
    "description": "Close.com PHP API client built on top of Guzzle 7",
    "keywords": [
        "api",
        "guzzle",
        "close"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Colin Longworth",
            "homepage": "https://www.highways.io"
        }
    ],
    "support": {
        "issues": "https://github.com/mckltech/close-php/issues",
        "source": "https://github.com/mckltech/close-php"
    },
    "autoload": {
        "psr-4": {
            "Close\\": [
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Close\\Test\\": [
                "test"
            ]
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "php-http/client-common": "^1.9 || ^2.0",
        "php-http/client-implementation": "*",
        "php-http/discovery": "^1.4",
        "php-http/httplug": "^1.0 || ^2.0",
        "php-http/message": "^1.7",
        "psr/http-message": "^1.0",
        "php-http/guzzle7-adapter": "*"
    },
    "require-dev": {
         "phpunit/phpunit": "^7.0",
         "squizlabs/php_codesniffer": "^3.1"
    },
    "config": {
        "sort-packages": true
    }
}
